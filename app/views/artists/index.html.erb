<div class="container">
	<div class="row">
		<div class="col-sm-12">
			<h1>Artistas</h1>
			<div class="text-right">
				<%= link_to "Publicar artista", new_artist_path, class:"btn btn-primary" %>
			</div>
			<table class="table">
				<rt>
					<tr>Nombre</tr>
					<tr>Url</tr>
				</rt>
				<% @artists.each do |artist| %>
			      <tr>
			      	<td><%= artist.name %></td>
			        <td><img src="<%= artist.image_url %>"></td>
			      </tr>
				<% end %>
			</table>
		</div>
	</div>
</div>

